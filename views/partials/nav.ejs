<%- include("header") %>

<!-- navbar section -->

    <nav class="navbar border-bottom navbar-expand-lg py-3 navbar-light bg-light">
        <div class="container">
            <a class="navbar-brand" href="#">
                <img src="<%= details.iconUrl %>" alt="" width="30" height="30" class="d-inline-block align-text-top">
                <%= details.name %>
            </a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
                aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav ms-auto mb-2 mb-lg-0">
                    <% details.links.forEach(detail=> { %>
                        <li class="nav-item">
                            <a class="nav-link active" aria-current="page" href="<%=detail.url %>">
                                <%= detail.label %>
                            </a>
                        </li>
                    <% }) %>
                </ul>
            </div>
        </div>
    </nav>